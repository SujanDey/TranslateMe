<Application x:Class="TranslateMe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="UI/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UI/Assets/Colors.xaml"/>
                <ResourceDictionary Source="UI/Assets/Controls.xaml"/>
                <ResourceDictionary Source="UI/Assets/DataGrid.xaml"/>
                <ResourceDictionary Source="UI/Assets/Menu.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
